<%= form_with model: event do |form| %>
  <%= form.label :name%>
  <%= form.text_field :name %>

  <%= form.label :location %>
  <%= form.text_field :location %>

  <%= form.label :date %>
  <%= form.date_field :date %>

  </br>

  <%= form.label :description %>
  </br>
  <%= form.text_area :description %>
  </br>
  <%= form.submit %>
<%end%>